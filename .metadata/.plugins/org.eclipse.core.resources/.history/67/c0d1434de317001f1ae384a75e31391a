package ch02;

public class ByteEx {

	public static void main(String[] args) {
		byte var1 = -128;
		byte var2 = -30;
		byte var3 = 0;
		byte var4 = 30;
		byte var5 = 127;
		
		System.out.println("var1->"+var1);
		System.out.println("var2->"+var2);
		System.out.println("var3->"+var3);
		System.out.println("var4->"+var4);
		System.out.println("var5->"+var5);


	}

}
