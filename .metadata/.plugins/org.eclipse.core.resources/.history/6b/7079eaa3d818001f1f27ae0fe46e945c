package ch04_02;
// 실습 IfDiceEx를 SwitchEx01로 Conversion(바꾸기)
public class SwEx01 {

	public static void main(String[] args) {
		int num = (int)(Math.random()*6) +1;
		
		switch (num) {
		case 1 :
			System.out.println("1이 나왔습니다.");
			break;
		case 2 :
			System.out.println("2이 나왔습니다.");
			break;
		}
	}

}
